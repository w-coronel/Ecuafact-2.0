@using Ecuafact.Web.Domain.Entities
@using Ecuafact.Web.Controllers;
@using Ecuafact.Web.MiddleCore.NexusApiServices;
@using System.Collections;

@model DashboardInfo
@{
    var carrier = false;
    var permmission = SessionInfo.UserRole == Ecuafact.Web.Models.SecuritySessionRole.Cooperative;
    if (permmission)
    {
        carrier = SessionInfo.UserSession?.Issuer?.IsCooperativeCarrier ?? false;
    }

    var _cantidad = "0";
    var _emitidos = 0;
    var _saldo = "0";
    var _cantidadDoc = 0;
    var _emitidoDoc = 0;
    var _saldoDoc = 0;
    var percentDocValue = 0.00;
    var percentDoc = "0.00%";
    var _colorDocmsg = "blue";
    var _colorDoc = "kt-bg-brand";
    var msgDoc = "";
    var verprogress = false;
    var verFechaExp = false;
    var verFechaFirma = false;
    if (!string.IsNullOrWhiteSpace(SessionInfo.UserSession?.Subscription?.AmountDocument))
    {
        _cantidad = SessionInfo.UserSession.Subscription.AmountDocument.Contains("0000000000") ? "Ilimitado" : SessionInfo.UserSession.Subscription.AmountDocument ?? "0";
        _emitidos = SessionInfo.UserSession.Subscription.IssuedDocument ?? 0;
        _saldo = SessionInfo.UserSession.Subscription.AmountDocument.Contains("0000000000") ? "Ilimitado" : $"{SessionInfo.UserSession.Subscription.BalanceDocument ?? 0}";

        _cantidadDoc = SessionInfo.UserSession.Subscription.AmountDocument.Contains("0000000000") ? 0 : Convert.ToInt32(SessionInfo.UserSession.Subscription.AmountDocument);
        _emitidoDoc = Convert.ToInt32(SessionInfo.UserSession.Subscription.IssuedDocument);
        _saldoDoc = SessionInfo.UserSession.Subscription.AmountDocument.Contains("0000000000") ? 0 : Convert.ToInt32(SessionInfo.UserSession.Subscription.BalanceDocument);
        if (_cantidadDoc > 0 && _saldoDoc > 0)
        {
            percentDocValue = (_emitidoDoc * 100) / _cantidadDoc; //_emitidoDoc * (100 / _cantidadDoc);
            percentDoc = string.Format("{0:0.00}%", percentDocValue);
        }
        if (percentDocValue >= 95)
        {
            _colorDocmsg = "red";
            _colorDoc = "kt-bg-danger";
            msgDoc = $"Documentos restan de";
        }
        else if (percentDocValue >= 0 && percentDocValue < 95)
        {
            msgDoc = $"Documentos emitidos de";
        }
        verprogress = !SessionInfo.UserSession.Subscription.AmountDocument.Contains("0000000000");

    }
    if(SessionInfo.UserSession?.Subscription != null)
    {
        verFechaExp = true;
    }
    if (SessionInfo.UserSession?.Issuer.CertificateExpirationDate != null)
    {
        verFechaFirma = true;
    }
    var userAvatar = Server.GetLogoFile(SessionInfo.UserSession.IssuerRUC);

    var tienePlan = SessionInfo.UserSession?.Subscription?.Status == Ecuafact.Web.Domain.Entities.SubscriptionStatusEnum.Activa ? $"Tu plan activado es {SessionInfo.UserSession?.Subscription?.LicenceType?.Name}" : " No tiene activo un plan.";
    var planIcono = SessionInfo.UserSession?.Subscription?.Status == Ecuafact.Web.Domain.Entities.SubscriptionStatusEnum.Activa ? "flaticon2-correct kt-font-success" : "flaticon2-warning kt-font-danger";
   


}
